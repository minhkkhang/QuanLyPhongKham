<Window x:Class="QuanLyPhongKham.TextDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:QuanLyPhongKham"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="TextDialog" Height="300" Width="300">
    <ScrollViewer>
        <TextBox Name="TxtBlock" TextWrapping="Wrap" IsReadOnly="True" Background="Transparent" BorderThickness="0"/>
    </ScrollViewer>
</Window>
